<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Reports::report">
                    <resource id="Opengento_WebapiLogger::reports"
                              title="Webapi Logs"
                              translate="title"
                              sortOrder="5">
                        <resource id="Opengento_WebapiLogger::reports_webapilogs"
                                  title="All Logs"
                                  translate="title"
                                  sortOrder="10"/>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
